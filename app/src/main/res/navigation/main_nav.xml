<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/main_nav"
app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/boardFragment"
        android:name="com.sharewithme.swm.fragments.BoardFragment"
        android:label="BoardFragment">
        <action
            android:id="@+id/action_boardFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_boardFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_boardFragment_to_modifyProfileFragment"
            app:destination="@id/modifyProfileFragment" />
        <action
            android:id="@+id/action_boardFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_boardFragment_to_declareFragment"
            app:destination="@id/declareFragment" />
    </fragment>
<fragment
    android:id="@+id/declareFragment"
    android:name="com.sharewithme.swm.fragments.DeclareFragment"
    android:label="DeclareFragment" >
    <action
        android:id="@+id/action_declareFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_declareFragment_to_boardFragment"
        app:destination="@id/boardFragment" />
    <action
        android:id="@+id/action_declareFragment_to_mapFragment"
        app:destination="@id/mapFragment" />
    <action
        android:id="@+id/action_declareFragment_to_modifyProfileFragment"
        app:destination="@id/modifyProfileFragment" />
    <action
        android:id="@+id/action_declareFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
</fragment>
<fragment
    android:id="@+id/homeFragment"
    android:name="com.sharewithme.swm.fragments.HomeFragment"
    android:label="HomeFragment" >
    <action
        android:id="@+id/action_homeFragment_to_boardFragment"
        app:destination="@id/boardFragment" />
    <action
        android:id="@+id/action_homeFragment_to_declareFragment"
        app:destination="@id/declareFragment" />
    <action
        android:id="@+id/action_homeFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
    <action
        android:id="@+id/action_homeFragment_to_modifyProfileFragment"
        app:destination="@id/modifyProfileFragment" />
    <action
        android:id="@+id/action_homeFragment_to_mapFragment"
        app:destination="@id/mapFragment" />
</fragment>
<fragment
    android:id="@+id/mapFragment"
    android:name="com.sharewithme.swm.fragments.MapFragment"
    android:label="fragment_map"
    tools:layout="@layout/fragment_map" >
    <action
        android:id="@+id/action_mapFragment_to_modifyProfileFragment"
        app:destination="@id/modifyProfileFragment" />
    <action
        android:id="@+id/action_mapFragment_to_boardFragment"
        app:destination="@id/boardFragment" />
    <action
        android:id="@+id/action_mapFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_mapFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
    <action
        android:id="@+id/action_mapFragment_to_declareFragment"
        app:destination="@id/declareFragment" />
</fragment>
<fragment
    android:id="@+id/modifyProfileFragment"
    android:name="com.sharewithme.swm.fragments.ModifyProfileFragment"
    android:label="ModifyProfileFragment" >
    <action
        android:id="@+id/action_modifyProfileFragment_to_boardFragment"
        app:destination="@id/boardFragment" />
    <action
        android:id="@+id/action_modifyProfileFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_modifyProfileFragment_to_declareFragment"
        app:destination="@id/declareFragment" />
    <action
        android:id="@+id/action_modifyProfileFragment_to_mapFragment"
        app:destination="@id/mapFragment" />
    <action
        android:id="@+id/action_modifyProfileFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
</fragment>
<fragment
    android:id="@+id/profileFragment"
    android:name="com.sharewithme.swm.fragments.ProfileFragment"
    android:label="ProfileFragment" >
    <action
        android:id="@+id/action_profileFragment_to_declareFragment"
        app:destination="@id/declareFragment" />
    <action
        android:id="@+id/action_profileFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_profileFragment_to_modifyProfileFragment"
        app:destination="@id/modifyProfileFragment" />
    <action
        android:id="@+id/action_profileFragment_to_mapFragment"
        app:destination="@id/mapFragment" />
    <action
        android:id="@+id/action_profileFragment_to_boardFragment"
        app:destination="@id/boardFragment" />
</fragment>
</navigation>